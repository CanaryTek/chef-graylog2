production:
  via: <%= node['GRAYLOG2']['EMAIL_TYPE'] %>
  host: <%= node['GRAYLOG2']['EMAIL_HOST'] %>
  enable_starttls_auto: <%= node['GRAYLOG2']['EMAIL_TLS'] %>
  port: <%= node['GRAYLOG2']['EMAIL_PORT'] %>
  auth: <%= node['GRAYLOG2']['EMAIL_AUTH'] %>
  user: <%= node['GRAYLOG2']['EMAIL_USER'] %>
  password: <%= node['GRAYLOG2']['EMAIL_PASSWD'] %>
  domain: <%= node['GRAYLOG2']['EMAIL_DOMAIN'] %>
